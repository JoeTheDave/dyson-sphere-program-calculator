{
  "name": "dyson-sphere-program-calculator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tailwind": "npx tailwindcss -i ./src/styles/index.css -o ./src/styles/tailwind.css",
    "build": "run-s \"build:*\"",
    "build:vite": "tsc && vite build",
    "build:css": "npm run tailwind -- --minify",
    "dev": "run-p \"dev:*\"",
    "dev:vite": "vite",
    "dev:css": "npm run tailwind -- --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "load-data": "tsx ./scripts/load-application-artifacts.ts",
    "prettier": "npx prettier --write ."
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "daisyui": "^4.7.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
